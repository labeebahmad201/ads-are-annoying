{
    "manifest_version": 3,
    "name": "Ads are annoying",
    "version": "1.0",
    "description": "Get rid of ads",
    "permissions": ["activeTab"],
    "action": {
      "default_icon": {},
      "default_title": "Ads are annoyings"
    },
    "icons": {},
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  